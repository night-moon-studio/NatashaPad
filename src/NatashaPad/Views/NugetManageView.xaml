             xmlns:local="clr-namespace:NatashaPad.Views"
             mc:Ignorable="d"
             d:DesignHeight="450"
             d:DesignWidth="800">
    <ContentControl Template="{StaticResource DialogWrapper}">
        <Grid>
            <Grid.RowDefinitions>
